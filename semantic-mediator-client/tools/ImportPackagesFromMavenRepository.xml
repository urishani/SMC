<?xml version = "1.0" encoding = "utf-8"?>
<project name = "SMC OAuth Library" default = "import" basedir = "..">
	<property name="lib" value="lib"/>
	<property name="lib-test" value="lib-test"/>
	<property name="maven" value="http://central.maven.org/maven2/"/>

	<target name="import">
		<delete dir="${lib}"/>
		<mkdir dir="${lib}"/>
		<get src="${maven}com/google/guava/guava/14.0.1/guava-14.0.1.jar"
			dest="${lib}/guava-14.0.1.jar"/>
		<get src="${maven}org/apache/httpcomponents/httpclient/4.2.2/httpclient-4.2.2.jar"
			dest="${lib}/httpclient-4.2.2.jar"/>
		<get src="${maven}org/apache/httpcomponents/httpcore/4.2.2/httpcore-4.2.2.jar"
			dest="${lib}/httpcore-4.2.2.jar"/>
		<get src="${maven}log4j/log4j/1.2.16/log4j-1.2.16.jar"
			dest="${lib}/log4j-1.2.16.jar"/>
		<get src="${maven}commons-logging/commons-logging/1.1.1/commons-logging-1.1.1.jar"
			dest="${lib}/commons-logging-1.1.1.jar"/>
		<delete dir="${lib-test}"/>
		<mkdir dir="${lib-test}"/>
		<get src="${maven}org/apache/jena/jena-iri/0.9.6/jena-iri-0.9.6.jar"
			dest="${lib-test}/jena-iri-0.9.6.jar"/>
		<get src="${maven}org/apache/jena/jena-core/2.10.1/jena-core-2.10.1.jar"
			dest="${lib-test}/jena-core-2.10.1.jar"/>
		<get src="${maven}org/slf4j/slf4j-log4j12/1.7.5/slf4j-log4j12-1.7.5.jar"
			dest="${lib-test}/slf4j-log4j12-1.7.5.jar"/>
		<get src="${maven}org/slf4j/slf4j-api/1.7.5/slf4j-api-1.7.5.jar"
			dest="${lib-test}/slf4j-api-1.7.5.jar"/>
	</target>
</project>

